<?xml version="1.0" encoding="utf-8" ?>
<content id="master">
  <html>
    <head>
      <meta name="Description" content="$description" />
      <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
      <meta name="Distribution" content="Global" />
      <meta name="Robots" content="index,follow" />
      <title>$page.Title</title>
      <link href="/Content/Site.css'" rel="stylesheet" type="text/css" />
    </head>
    <body>
      <!-- wrap starts here -->
      <div id="wrap">
        <div id="header">
          <div id="header-content">
            <h1 id="logo">
              <a href="/" title="Laan Family Site">Laan<span class="gray">Family</span></a>
            </h1>
            <h2 id="slogan">$page.Title</h2>
            <!-- Menu Tabs -->
            <list data="home.Pages"/>
          </div>
        </div>
        <div class="headerphoto">
        </div>

        <!-- content-wrap starts here -->
        <div id="content-wrap">
          <div id="content">
            <div id="sidebar">
              <zone name="site\sidebar"/>
            </div>
            <div id="main">
              <zone id="main"/>
            </div>
          </div>
        </div>
        <div id="footer">
          <zone name="site\footer"/>
        </div>
      </div>
    </body>
  </html>
</content>
